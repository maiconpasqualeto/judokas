	<queryString>
		<![CDATA[SELECT 
	u.nome agremiacao,
	a.id,
	a.nome nome_atleta,
	a.data_nascimento,
	a.matricula_zempo,
	a.data_emissao_carteira
FROM atleta a
	inner join usuario u ON 
	 a.id_usuario = u.id 
WHERE 
	 a.status_registro = 'A' 
	 AND a.data_emissao_carteira IS NOT NULL  
ORDER BY u.nome DESC,
	a.nome DESC]]>
	</queryString>
